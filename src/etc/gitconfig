    symlinks = false
    autocrlf = true
    fscache = true
    editor = nano
[color]
    diff = auto
    status = auto
    branch = auto
	interactive = true
[help]
    format = html
[http]
    sslBackend = schannel
[diff "astextplain"]
    textconv = astextplain
[rebase]
    autosquash = true
[filter "lfs"]
    clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
    required = true
[credential]
[pull]
    rebase = false
